package gci.translate;
import gci.tree.*;

/**
 *
 * @author Caio Fellipe
 */
public class Ex {
    private Expression expr;
    
    public Ex(Expression expr)
    {
        this.expr = expr;
    }

    public Expression unEx()
    {
        return expr;
    }

    public Stm unNx()
    {
        return new EXP(expr);
    }
}
